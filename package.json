    "rxcollection-subscriber": "^1.0.18",
    "rxdb": "^10.0.3",
    "rxjs": "^7.3.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "cashify": "^2.5.0",
    "deepmerge": "^4.2.2",
